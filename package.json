{
  "name": "api-meteorologia-typescript",
  "version": "1.0.0",
  "description": "Complete API built with Node.js and Typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node dist/src/index.js",
    "dev": "nodemon --exec ts-node-dev --transpile-only src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test:functional": "jest --projects ./test --forceExit --runInBand",
    "test:unit": "jest",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EderneyJunior/api-meteorologia-typescript"
  },
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Ederney Pedroso",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EderneyJunior/api-meteorologia-typescript/issues"
  },
  "homepage": "https://github.com/EderneyJunior/api-meteorologia-typescript#readme",
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@types/express": "^4.17.13",
    "axios": "^1.7.2",
    "body-parser": "^1.19.1",
    "express": "^4.17.2",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/module-alias": "^2.0.1",
    "@types/node": "16",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "supertest": "^6.1.6",
    "ts-jest": "^29.2.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^5.5.4"
  },
  "directories": {
    "test": "test"
  }
}
